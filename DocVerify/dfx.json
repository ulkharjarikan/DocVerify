{
  "canisters": {
    "docverify": {
      "type": "custom",
      "main": "src/index.ts",
      "candid": "src/docverify.did",
      "candid_gen": "http",
      "build": "npx azle docverify",
      "wasm": ".azle/docverify/docverify.wasm",
      "gzip": true,
      "metadata": [
        {
          "name": "candid:service",
          "path": "src/docverify.did"
        },
        {
          "name": "cdk:name",
          "content": "azle"
        }
      ]
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "version": 1
}
